{
  "name": "alex-qiu/aliyun-core",
  "description": "AliYun Core For Hyperf",
  "keywords": [
    "php",
    "swoole",
    "hyperf",
    "aliyun",
    "core"
  ],
  "require": {
    "php": ">=8.0",
    "hyperf/contract": "3.0.*",
    "hyperf/guzzle": "3.0.*",
    "hyperf/server": "3.0.*",
    "psr/container": "^1.0|^2.0"
  },
  "require-dev": {
    "php": ">=8"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "alex",
      "email": "alex.qiubo@qq.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ym\\AliyunCore\\": "src/"
    },
    "files": [
      "src/Config.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "YmTest\\AliyunCore\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "hyperf": {
      "config": "Ym\\AliyunCore\\ConfigProvider"
    }
  }
}
